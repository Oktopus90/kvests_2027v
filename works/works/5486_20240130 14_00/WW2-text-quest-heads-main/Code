awnser = "1"
while awnser == "1":
    print("""–ó–¥—Ä–∞–≤—Å—Ç–≤—É–π—Ç–µ
    –í—ã - –Ω–∞–≤–æ–¥—á–∏–∫ —Ç–∞–Ω–∫–∞ pz.kmpf.w.III E.
    –ù–∞ –¥–≤–æ—Ä–µ 05:00 10 —Å–µ–Ω—Ç—è–±—Ä—è 1938.
    –ù–∞—Ö–æ–¥–∏—Ç–µ—Å—å –≤—ã –≤ –ü–æ–ª—å—Å–∫–æ–º –≥–æ—Ä–æ–¥–µ –ö–æ–º–∏–∫.
    –í–∞—à–∞ –∑–∞–¥–∞—á–∞ –∑–∞–∫–ª—é—á–∞–µ—Ç—Å—è –≤ –∑–∞—Ö–≤–∞—Ç–µ –≥–æ—Ä–æ–¥–∞ –ü–æ–∑–Ω–∞–Ω—å""")
    print("""–í –∫–∞–∫–æ–π –≥–æ—Ä–æ–¥ –µ–¥–µ–º?
    (1) –ü–æ–∑–Ω–∞–Ω—å
    (2) –õ–æ–¥–∑—å""")
    target_town = input()
    if target_town != "1" or target_town != "2":
        t_t_awnser = 0
        while t_t_awnser == 0:
            print("–ù–µ—Ç —Ç–∞–∫–æ–≥–æ –≤–∞—Ä–∏–∞–Ω—Ç–∞")
            print("–í–≤–µ–¥–∏—Ç–µ –µ—â—ë —Ä–∞–∑")
            target_town = input()
            if target_town == "1" or target_town == "2":
                t_t_awnser = 1
    if target_town == "1":
        print("–û—Ç–ª–∏—á–Ω–æ–µ —Ä–µ—à–µ–Ω–∏–µ!")
        print("""–í—ã –µ–¥–µ—Ç–µ —É–∂–µ 2 —á–∞—Å–∞ –∏ –≤—Å—Ç—Ä–µ—á–∞–µ—Ç–µ —Ç–∞–Ω–∫–µ—Ç–∫—É –ø–æ–¥ –Ω–∞–∑–≤–∞–Ω–∏–µ–º –¢–ö-2
        –ß—Ç–æ –≤—ã –±—É–¥–µ—Ç–µ –¥–µ–ª–∞—Ç—å?
        (1) –ù–µ –æ—Ç–∫—Ä—ã–≤–∞—Ç—å –æ–≥–æ–Ω—å
        (2) –û—Ç–∫—Ä—ã—Ç—å –æ–≥–æ–Ω—å""")
        TK_2_awnser = input()
        if TK_2_awnser != "1" or TK_2_awnser != "2":
            TK_2_awnser_2 = 0
            while TK_2_awnser_2 != "1" or TK_2_awnser != "2":
                print("–ù–µ—Ç —Ç–∞–∫–æ–≥–æ –≤–∞—Ä–∏–∞–Ω—Ç–∞")
                print("–≤–≤–µ–¥–∏—Ç–µ –µ—â—ë —Ä–∞–∑")
                TK_2_awnser = input()
                if TK_2_awnser == "1" or TK_2_awnser == "2":
                    TK_2_awnser_2 = 1
        if TK_2_awnser == "1":
            print("–¢–∞–Ω–∫–µ—Ç–∫–∞ —Å—Ç—Ä–µ–ª—è–µ—Ç –ø–æ –≤–∞–º!")
            TK_2_set = {"a1", "b1", "c1", "d1"}
            random1 = TK_2_set.pop()
            if "a1" not in random1:
                print("""–í–∞—à—É —Ç–µ—Ö–Ω–∏–∫—É —É–Ω–∏—á—Ç–æ–∂–∏–ª–∏.
                –í—ã –ø—Ä–æ–∏–≥—Ä–∞–ª–∏.""")
            else:
                print("""–í–∞—à–∞ —Ç–µ—Ö–Ω–∏–∫–∞ –Ω–µ –ø–æ—Å—Ç—Ä–æ–¥–∞–ª–∞""")
                TK_2_awnser = "2"
        if TK_2_awnser == "2":
            print("–í—ã —Å—Ç—Ä–µ–ª—è–µ—Ç–µ –ø–æ —Ç–∞–Ω–∫–µ—Ç–∫–µ!")
            TK_2_set = {"a2", "b2", "c2"}
            random2 = TK_2_set.pop()
            if "a2" not in TK_2_set:
                print("""–í—ã –Ω–µ –ø—Ä–æ–±–∏–ª–∏ –¢–ö-2
                –ù–æ –≤–¥—Ä—É–≥ –∫ –≤–∞–º –Ω–∞ –ø–æ–º–æ—â—å –≤—ã–µ–∑–∂–∞–µ—Ç –Ω–µ—Å–∫–æ–ª—å–∫–æ —Ç–∞–Ω–∫–æ–≤ LT-35.
                –û–Ω–∏ –ø–æ–º–æ–≥–∞—é—Ç —É–Ω–∏—á—Ç–æ–∂–∏—Ç—å –≤–∞–º —Ç–∞–Ω–∫–µ—Ç–∫—É.""")
            else:
                print("""–í—ã —É–Ω–∏—á—Ç–æ–∂–∏–ª–∏ –¢–ö-2!
                –ù–æ –Ω–µ–æ–∂–∏–¥–∞–Ω–Ω–æ –∫ –≤–∞–º –ø–æ–¥–µ–∑–∂–∞—é—Ç –Ω–µ—Å–∫–æ–ª—å–∫–æ —Ç–∞–Ω–∫–æ–≤ LT-35.""")
        print("–ö—É–¥–∞ –µ–¥–µ–º?")
        print("(1) –ü–æ–∑–Ω–∞–Ω—å")
        town = input()
        while town != "1" or town != "–ü–æ–∑–Ω–∞–Ω—å":
            print("–û—Ç–≤–µ—Ç –Ω–µ–≤–µ—Ä–Ω—ã–π")
            print("""–ö—É–¥–∞ –µ–¥–µ–º?
            (1) –ü–æ–∑–Ω–∞–Ω—å""")
        print("""–û—Ç–ª–∏—á–Ω–æ–µ —Ä–µ—à–µ–Ω–∏–µ!
        –°–µ–π—á–∞—Å 09:00 –∏ –≤—ã –¥–æ–µ–∑–∂–∞–µ—Ç–µ –¥–æ –ü–æ–∑–Ω–∞–Ω–∏—é
        –¢–∞–º –≤—ã –≤—Å—Ç—Ä–µ—á–∞–µ—Ç–µ 3 —Ç–∞–Ω–∫–∞ Vickers-6.
        –í–∞—à–∏ —Å–æ—é–∑–Ω–∏–∫–∏ —É–∂–µ —É–Ω–∏—á—Ç–æ–∂–∏–ª–∏ –æ–¥–∏–Ω —Ç–∞–Ω–∫.
        –ß—Ç–æ –≤—ã —Å–¥–µ–ª–∞–µ—Ç–µ?
        (1) –°—Ç—Ä–µ–ª—è—Ç—å –ø–æ –≤—Ä–∞–≥–∞–º
        (2) –°—Ç—Ä–µ–ª—è—Ç—å –ø–æ —Å–≤–æ–∏–ºüíÄ
        (3) –û—Ç—ä–µ—Ö–∞—Ç—å""")
        Vickers_6_awnser = input()
        while (Vickers_6_awnser != "1" or Vickers_6_awnser != "2") or (Vickers_6_awnser != "3" or Vickers_6_awnser != "Ratte"):
            print("""–ù–µ—Ç —Ç–∞–∫–æ–π –æ–ø—Ü–∏–∏
            –ß—Ç–æ –≤—ã —Å–¥–µ–ª–∞–µ—Ç–µ?
            (1) –°—Ç—Ä–µ–ª—è—Ç—å –ø–æ –≤—Ä–∞–≥–∞–º
            (2) –°—Ç—Ä–µ–ª—è—Ç—å –ø–æ —Å–≤–æ–∏–ºüíÄ
            (3) –û—Ç—ä–µ—Ö–∞—Ç—å""")
            Vickers_6_awnser = input()
        if Vickers_6_awnser == "1":
            print("–í—ã —Å—Ç—Ä–µ–ª—è–µ—Ç–µ –≤ —Ç–∞–Ω–∫.")
            Vickers_6_set = {"b1", "b2"}
            if "b1" not in Vickers_6_set:
                print("""–í—ã –Ω–µ –ø—Ä–æ–±–∏–ª–∏ Vickres.
                –û–Ω —Å—Ç—Ä–µ–ª—è–µ—Ç –≤ –≤–∞—à —Ç–∞–Ω–∫.
                –í—ã –ø—Ä–æ–∏–≥—Ä–∞–ª–∏""")
            else:
                print("""–í—ã —É–Ω–∏—á—Ç–æ–∂–∏–ª–∏ Vickres.
                –í –ø–æ—Å–ª–µ–¥—Å—Ç–≤–∏–∏ –≤—ã —É–Ω–∏—á—Ç–æ–∂–∏–ª–∏ –≤—Å–µ —Ç–∞–Ω–∫–∏ –∏ –∑–∞—Ö–≤–∞—Ç–∏–ª–∏ –≥–æ—Ä–æ–¥.
                –í—ã –ø–æ–±–µ–¥–∏–ª–∏.""")
        elif Vickers_6_awnser == "2":
            print("""–ü–æ–∑–¥—Ä–∞–≤–ª—è–µ–º! –í—ã - –∏–¥–∏–æ—Ç
            –í–∞—à–∏ —Å–æ—é–∑–Ω—ã–µ —Ç–∞–Ω–∫–∏–±—ã–ª–∏ —É–Ω–∏—á—Ç–æ–∂–µ–Ω—ã.
            –ù–æ —Å—Ä–∞–∑—É –∂–µ –ø–æ—Å–ª–µ —É–Ω–∏—á—Ç–æ–∂–µ–Ω–∏—è —Ç–∞–Ω–∫–æ–≤ —É–Ω–∏—á—Ç–æ–∂–∞—é—Ç –≤–∞—Å!
            –í—ã –ø—Ä–æ–∏–≥—Ä–∞–ª–∏""")
        elif Vickers_6_awnser == "3":
            print("""–í—ã —Ä–µ—à–∏–ª–∏ –æ—Ç—ä–µ—Ö–∞—Ç—å –∏ –∞—Ç–∞–∫–æ–≤–∞—Ç—å –ø–æ–∑–∂–µ.
            –£–∂–µ –≤ 10:00 –∫ –≤–∞–º –≤ –ø–æ–¥–¥–µ—Ä–∂–∫—É –ø–æ–¥—ä–µ—Ö–∞–ª–æ –Ω–µ—Å–∫–æ–ª—å–∫–æ —Ç–∞–Ω–∫–æ–≤ pz.kmpf.w.II C.
            –í –∏—Ç–æ–≥–µ –≤—ã —É–Ω–∏—á—Ç–æ–∂–∏–ª–∏ —Ç–∞–Ω–∫–∏ –∏ –∑–∞—Ö–≤–∞—Ç–∏–ª–∏ –ü–æ–∑–Ω–∞–Ω—å
            –í—ã –ø–æ–±–µ–¥–∏–ª–∏.""")
        elif Vickers_6_awnser == "Ratte":
            print("""–ü–æ–∑–¥—Ä–∞–≤–ª—è–µ–º! –í—ã –Ω–∞—à–ª–∏ —Å–µ–∫—Ä–µ—Ç–Ω—É—é –∫–æ–Ω—Ü–æ–≤–∫—É!
            –ö –≤–∞–º –ø–æ–¥—ä–µ–∑–∂–∞–µ—Ç Ratte (–∑–¥–µ—Å—å Ratte –±—ã–ª —Å–æ–∑–¥–∞–Ω –≤ 1939 –≥–æ–¥—É) –∏ –æ–Ω —É–Ω–∏—á—Ç–æ–∂–∞–µ—Ç –≤—Å–µ —Ç–∞–Ω–∫–∏.
            –° –µ–≥–æ –ø–æ–º–æ—â–∏ –≤—ã –∑–∞—Ö–≤–∞—Ç–∏–ª–∏ –≥–æ—Ä–æ–¥ –±–µ–∑ –ø—Ä–æ–±–ª–µ–º.
            –í —Å–∫–æ—Ä–æ–º –≤—Ä–µ–º–µ–Ω–∏ —ç—Ç–æ—Ç —Ç–∞–Ω–∫ –ø–æ–µ–¥–µ—Ç –≤ –°–°–°–†
            –ö–æ–¥ –¥–ª—è –∞–∫—Ç–∏–≤–∞—Ü–∏–∏ Ratte –≤ —Å—Ü–µ–Ω–∞—Ä–∏–∏ –ì–µ—Ä–º–∞–Ω–∏–∏ 1941 –≥–æ–¥–∞: RaTtE193919412""")
    elif target_town == "2":
        print("–û—Ç–ª–∏—á–Ω–æ–µ —Ä–µ—à–µ–Ω–∏–µ!")
        print("–ñ–∞–ª—å —ç—Ç–æ—Ç –≥–æ—Ä–æ–¥ –Ω–µ –≥–æ—Ç–æ–≤:(")
    print("""–•–æ—Ç–∏—Ç–µ –ª–∏ –≤—ã —Å—ã–≥—Ä–∞—Ç—å –µ—â—ë —Ä–∞–∑?
    (1) –¥–∞
    (2) –Ω–µ—Ç""")
    awnser = input()
    if awnser != "–¥–∞" or awnser != "–Ω–µ—Ç":
        awnser_2 = 0
        while awnser != "1" or awnser != "2":
            print("""–≠—Ç–æ –Ω–µ –æ—Ç–≤–µ—Ç.
            –•–æ—Ç–∏—Ç–µ –ª–∏ –≤—ã —Å—ã–≥—Ä–∞—Ç—å –µ—â—ë —Ä–∞–∑?
            (1) –¥–∞"
            (2) –Ω–µ—Ç""")
            awnser = input()
            if awnser == "–¥–∞" or awnser == "–Ω–µ—Ç":
                awnser_2 = 1
